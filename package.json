{
	"name": "@nftchance/plug-router",
	"version": "0.0.0",
	"description": "The intent router for newly created intents.",
	"scripts": {
		"predev:db": "tsx ./src/lib/scripts/db.ts",
		"predev:prisma": "pnpm prisma:migrate && pnpm prisma:push",
		"predev": "run-s predev:*",
		"dev:express": "tsx watch ./src/server/index.ts",
		"dev": "run-p dev:*",
		"prisma:generate": "pnpm prisma generate",
		"prisma:migrate": "pnpm prisma migrate dev",
		"prisma:push": "pnpm prisma db push",
		"prisma:pull": "pnpm prisma db pull",
		"prisma:seed": "pnpm prisma db seed",
		"prisma:reset": "pnpm prisma db push --force-reset && pnpm prisma db push",
		"format": "prettier --write ."
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^5.16.1",
		"@trpc/client": "^10.45.2",
		"@trpc/server": "^10.45.2",
		"axios": "^1.7.2",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"viem": "^2.17.2",
		"ws": "^8.18.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^20.14.10",
		"@types/ws": "^8.5.10",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.2",
		"prisma": "^5.16.1",
		"superjson": "^2.2.1",
		"tsx": "^4.16.2",
		"typescript": "^5.5.3"
	}
}
